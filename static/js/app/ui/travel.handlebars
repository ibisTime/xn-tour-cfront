{{#each items}}
	<div class="item">
		<a class="wp100 hp100" href="../travel/travel-detail.html?code={{code}}">
		    <div class="item-c-div item-l">
		        <img class="center-img" src="{{formatImage pathPic}}"/>
		    </div>
		    <div class="item-c">
		        <div class="item-c-top1 twoline-ellipsis pb0">{{name}}</div>
		        <div class="index-item-bottom">{{joinPlace}}出发</div>
		    </div>
		    <div class="item-c-div item-r item-r1">¥{{formatMoney price}}<span class="smf">起</span></div>
		</a>
	</div>
{{/each}}