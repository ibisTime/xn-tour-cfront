{{#each items}}
	<div class="item">
		<a class="wp100 hp100" href="#?code={{code}}">
		    <div class="item-c-div item-l">
		        <img class="center-img" src="{{formatImage pic}}">
		    </div>
		    <div class="item-c">
		        <div class="item-c-top1 c_00 t_norwrap">{{name}}</div>
		        <div class="item-m-m twoline-ellipsis">{{description}}</div>
		        <div class="travel-item-bottom">{{formatePointDate publishDatetime}}</div>
		    </div>
		</a>
	</div>
{{/each}}