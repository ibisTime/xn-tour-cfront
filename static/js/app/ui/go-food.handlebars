{{#each items}}
    <div class="item">
        <a class="block_wp100" href="../go/food-detail.html?code={{code}}">
            <div class="item-c-div item-l">
                <img class="center-img" src="{{formatImage pic}}"/>
            </div>
            <div class="item-c">
                <div class="item-c-top1 t_norwrap">{{name}}</div>
                <div class="item-c-center item-c-center1 t_norwrap">{{detail}}</div>
            </div>
            <div class="item-c-div item-r item-r1">{{price}}</div>
        </a>
    </div>
{{/each}}
